# environment
TERM="xterm-256color"
ALTERNATE_EDITOR=""
EDITOR="emacsclient -c -n"

# GitHub API
github_token="$HOME/.github"
[ -e $github_token ] && source $github_token

# homebrew GitHub API
homebrew_token="$HOME/.homebrew"
[ -e $homebrew_token ] && source $homebrew_token

# boxen
[ -e /opt/boxen/env.sh ] && source /opt/boxen/env.sh

# path
path_front "bin"
path_back "/sbin" "/bin" "/usr/sbin" "/usr/bin" "/usr/local/bin" "/usr/texbin" "~/bin"

# prompt
source ~/.liquidprompt/liquidprompt
